[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f

[core]
	excludesfile = ~/.gitignore_global
	quotePath = false
	commitGraph = true

[credential]
	helper = osxkeychain

[pull]
	rebase = true

[init]
	defaultBranch = main

[receive]
	advertisePushOptions = true
	procReceiveRefs = refs/for

[gc]
	writeCommitGraph = true
